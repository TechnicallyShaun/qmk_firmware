{
    "keyboard_name": "Keychron C1 Pro 8K",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "Keychron",
    "processor": "AT32F405",
    "bootloader": "at32-dfu",
    "usb": {
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "dip_switch": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true
    },
    "matrix_pins": {
        "cols": ["C7", "C8", "A14", "A15", "C10", "C11", "C13", "C14", "C15", "C0", "C1", "C2", "C3", "A0", "A1", "A2", "A3"],
        "rows": ["C12", "D2", "B3", "B4", "B5", "B6"]
    },
    "diode_direction": "COL2ROW",
    "dip_switch" :{
        "pins": ["B1"]
    },
    "indicators": {
        "caps_lock": "B10"
    },
    "build": {
        "debounce_type": "custom"
    },
    "debounce": 30,
    "eeprom": {
        "wear_leveling": {
            "driver": "embedded_flash",
            "logical_size": 3072,
            "backing_size": 6144
        }
    }
}
